<p id="notice"><%= notice %></p>

<h1>Pages</h1>

<table>
  <thead>
    <tr>
      <th>Home</th>
      <th>Attendance</th>
      <th>Therapist</th>
      <th>Price</th>
      <th>Event</th>
      <th>Access</th>
      <th>Recruitment</th>
      <th>Diary</th>
      <th>Movie</th>
      <th>Ranking</th>
      <th>Reserve</th>
      <th>Mail</th>
      <th>Inquiry</th>
      <th>Link</th>
      <th>Privacy</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @pages.each do |page| %>
      <tr>
        <td><%= page.home %></td>
        <td><%= page.attendance %></td>
        <td><%= page.therapist %></td>
        <td><%= page.price %></td>
        <td><%= page.event %></td>
        <td><%= page.access %></td>
        <td><%= page.recruitment %></td>
        <td><%= page.diary %></td>
        <td><%= page.movie %></td>
        <td><%= page.ranking %></td>
        <td><%= page.reserve %></td>
        <td><%= page.mail %></td>
        <td><%= page.inquiry %></td>
        <td><%= page.link %></td>
        <td><%= page.privacy %></td>
        <td><%= link_to 'Show', page %></td>
        <td><%= link_to 'Edit', edit_page_path(page) %></td>
        <td><%= link_to 'Destroy', page, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Page', new_page_path %>
