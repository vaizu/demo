<p id="notice"><%= notice %></p>

<h1>Settings</h1>

<table>
  <thead>
    <tr>
      <th>Login</th>
      <th>Login pass</th>
      <th>Customer tel</th>
      <th>Recruiting tel</th>
      <th>Mail</th>
      <th>Opening</th>
      <th>Closing</th>
      <th>Reserve</th>
      <th>Holiday</th>
      <th>Nearest</th>
      <th>Address</th>
      <th>Business trip</th>
      <th>Cast ja</th>
      <th>Cast en</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @settings.each do |setting| %>
      <tr>
        <td><%= setting.login_id %></td>
        <td><%= setting.login_pass %></td>
        <td><%= setting.customer_tel %></td>
        <td><%= setting.recruiting_tel %></td>
        <td><%= setting.mail %></td>
        <td><%= setting.opening %></td>
        <td><%= setting.closing %></td>
        <td><%= setting.reserve %></td>
        <td><%= setting.holiday %></td>
        <td><%= setting.nearest %></td>
        <td><%= setting.address %></td>
        <td><%= setting.business_trip %></td>
        <td><%= setting.cast_ja %></td>
        <td><%= setting.cast_en %></td>
        <td><%= link_to 'Show', setting %></td>
        <td><%= link_to 'Edit', edit_setting_path(setting) %></td>
        <td><%= link_to 'Destroy', setting, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Setting', new_setting_path %>
